{
  "manifest_version": 2,

  "name": "Block Pinterest",
  "description": "Block pinterest from google image search results.",
  "version": "1.0",


  "icons": {
    "128": "img/large.png",
    "48": "img/medium.png"
  },

  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}
